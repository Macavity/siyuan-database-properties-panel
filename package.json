{
  "name": "siyuan-database-properties-panel",
  "version": "1.7.0",
  "type": "module",
  "description": "Plugin for Siyuan (https://b3log.org/siyuan). Provides a panel to view database properties of a page created from a database.",
  "repository": "https://github.com/Macavity/siyuan-database-properties-panel",
  "homepage": "https://github.com/Macavity/siyuan-database-properties-panel",
  "author": "Macavity",
  "license": "MIT",
  "scripts": {
    "make-link": "node --no-warnings ./scripts/make_dev_link.js",
    "make-link-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/elevate.ps1 -scriptPath ./scripts/make_dev_link.js",
    "dev": "NODE_ENV=development vite build --watch",
    "dev:run": "NODE_ENV=development vite build",
    "update-version": "node --no-warnings ./scripts/update_version.js",
    "build": "NODE_ENV=production vite build",
    "build:install": "NODE_ENV=production vite build && node --no-warnings ./scripts/make_install.js",
    "build:install:dev": "NODE_ENV=development vite build && node --no-warnings ./scripts/make_install.js",
    "check-i18n": "node ./scripts/check_i18n.js",
    "lint": "eslint",
    "test": "vitest",
    "make-install": "vite build && node --no-warnings ./scripts/make_install.js"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@sentry/browser": "^8.55.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@tsconfig/svelte": "^4.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.23",
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "dayjs": "^1.11.19",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-svelte": "^2.46.1",
    "fast-glob": "^3.3.3",
    "glob": "^11.1.0",
    "globals": "^15.15.0",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23",
    "minimist": "^1.2.8",
    "prettier": "^3.8.0",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.97.2",
    "semver": "^7.7.3",
    "siyuan": "^1.1.7",
    "siyuan-app": "github:siyuan-note/siyuan#v3.5.0",
    "svelte": "^5.46.4",
    "svelte-eslint-parser": "^0.42.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^5.4.21",
    "vite-plugin-static-copy": "3.1.2",
    "vite-plugin-zip-pack": "^1.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/semver": "^7.7.1",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^20.3.1",
    "jsdom": "^25.0.1",
    "vitest": "^3.2.4"
  }
}